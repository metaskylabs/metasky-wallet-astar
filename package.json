{
  "name": "metasky-wallet",
  "description": "A web3 wallet offering comprehensive set of features by leveraging XRPL technology",
  "version": "1.0.0",
  "private": true,
  "author": "Metasky",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": " NODE_OPTIONS=--max-old-space-size=8192 next -p 3005",
    "build": "rm -rf ./build && NODE_OPTIONS=--max-old-space-size=8192 next build",
    "build:staging": "cp .env.development .env.production && yarn build",
    "start:staging": "env-cmd -f .env.development yarn start",
    "start": "next start -p 8080",
    "start:prod": "env-cmd -f .env.production next start -p 3005",
    "build:prod": "env-cmd -f .env.production next build",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@beam-australia/react-env": "3.1.1",
    "@elastic/apm-rum": "^5.12.0",
    "@elastic/apm-rum-react": "^1.4.2",
    "@emotion/babel-plugin": "11.9.2",
    "@emotion/react": "11.7.1",
    "@json-rpc-tools/utils": "1.7.6",
    "@next/bundle-analyzer": "12.1.5",
    "@tanstack/react-query": "4.2.1",
    "amplitude-js": "8.18.4",
    "axios": "0.25.0",
    "bootstrap": "5.1.3",
    "env-cmd": "10.1.0",
    "formik": "2.2.9",
    "framer-motion": "6.2.8",
    "html-react-parser": "3.0.8",
    "js-file-download": "0.4.12",
    "jwt-decode": "3.1.2",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "next": "12.0.9",
    "next-pwa": "5.5.2",
    "preact": "10.17.1",
    "qr-scanner": "1.4.2",
    "qrcode": "1.5.0",
    "rc-slider": "10.0.0",
    "react": "17.0.2",
    "react-bootstrap": "2.4.0",
    "react-date-picker": "8.4.0",
    "react-device-detect": "2.2.2",
    "react-dom": "17.0.2",
    "react-hot-toast": "2.2.0",
    "react-howler": "5.2.0",
    "react-lottie": "1.2.3",
    "react-markdown": "8.0.4",
    "react-pin-input": "1.0.1",
    "react-qr-reader": "3.0.0-beta-1",
    "react-redux": "8.0.2",
    "react-router-dom": "^6.4.2",
    "react-zoom-pan-pinch": "2.1.3",
    "redux": "4.2.0",
    "redux-thunk": "2.4.1",
    "rxjs": "7.8.1",
    "sharp": "0.31.1",
    "siwe": "1.1.6",
    "swiper": "8.0.3",
    "universal-cookie": "6.1.0",
    "valtio": "1.7.5",
    "video.js": "7.21.1"
  },
  "devDependencies": {
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@google-cloud/storage": "^6.11.0",
    "@types/amplitude-js": "8.16.1",
    "@types/jwt-decode": "3.1.0",
    "@types/lodash": "4.14.182",
    "@types/moment": "2.13.0",
    "@types/node": "16.11.21",
    "@types/qrcode": "1.4.2",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-howler": "5.2.0",
    "@types/react-lottie": "1.2.6",
    "@types/react-qr-reader": "2.1.4",
    "@types/react-redux": "7.1.24",
    "@types/react-tooltip": "4.2.4",
    "@types/redux": "3.6.0",
    "@types/redux-thunk": "2.1.0",
    "@types/video.js": "7.3.50",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.9",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.2",
    "postinstall-postinstall": "2.1.0",
    "prettier": "2.5.1",
    "react-env": "0.0.0",
    "redux-devtools-extension": "2.13.9",
    "typescript": "4.9.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
